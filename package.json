{
  "name": "@docamz/json-tokenizer",
  "version": "1.5.0",
  "description": "Lightweight JSON tokenizer with multiple encoding strategies (alphabetic, numeric, base64, UUID, custom) for compression and optimization",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "require": "./dist/react/index.js",
      "default": "./dist/react/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "dev": "tsc --watch"
  },
  "keywords": [
    "json",
    "tokenizer",
    "compression",
    "dictionary",
    "typescript",
    "alphabetic",
    "numeric",
    "base64",
    "uuid",
    "encoding",
    "optimization",
    "react",
    "browser",
    "nodejs",
    "SSR"
  ],
  "author": "DocAmz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DocAmz/json-tokenizer.git"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^18.3.28",
    "@vitejs/plugin-react": "^4.7.0",
    "jsdom": "^25.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  }
}
